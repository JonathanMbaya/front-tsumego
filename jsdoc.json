{
    "source": {
      "include": ["src"],
      "includePattern": ".jsx?$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "node_modules/docdash"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }
